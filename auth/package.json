    "@types/lodash.groupby": "^4.6.9",
    "@types/node": "^20.12.7",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "jest": "^29.7.0",
    "pg-mem": "^2.8.1",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5"
  },
